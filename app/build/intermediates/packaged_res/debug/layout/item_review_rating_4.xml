        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_review_rating_4_layout_7"
        android:layout_marginTop="10dp"
        >

        <RelativeLayout
            android:id="@+id/item_review_rating_4_layout_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >

            <TextView
                android:id="@+id/item_review_rating_4_text_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1"
